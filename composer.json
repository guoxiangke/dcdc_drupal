{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "dale",
            "role": "dale.tel@139.com"
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        "jquery-bar-rating":{
          "type": "package",
          "package": {
            "name": "antennaio/jquery-bar-rating",
            "version": "1.2.2",
            "type": "drupal-library",
            "dist": {
              "type": "zip",
              "url": "https://github.com/antennaio/jquery-bar-rating/archive/v1.2.2.zip"
            }
          }
        },
        "php-ga":{
          "type": "package",
          "package": {
            "name": "thomasbachem/php-ga",
            "version": "1.2",
            "type": "drupal-library",
            "dist": {
              "type": "zip",
              "url": "https://github.com/thomasbachem/php-ga/archive/v1.2.zip"
            }
          }
        },
        "htmlpurifier":{
          "type": "package",
          "package": {
            "name": "htmlpurifier",
            "version": "4.10.0",
            "type": "drupal-library",
            "dist": {
              "type": "zip",
              "url": "http://htmlpurifier.org/releases/htmlpurifier-4.10.0.zip"
            }
          }
        },
        "simplehtmldomp":{
          "type": "package",
          "package": {
            "name": "simplehtmldom",
            "version": "1.5",
            "type": "drupal-library",
            "dist": {
              "type": "zip",
              "url": "https://jaist.dl.sourceforge.net/project/simplehtmldom/simplehtmldom/1.5/simplehtmldom_1_5.zip"
            }
          }
        }
    },
    "require": {
        "antennaio/jquery-bar-rating": "^1.2",
        "aws/aws-sdk-php": "^3.60",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/audiofield": "^1.6",
        "drupal/backup_migrate": "^4.0",
        "drupal/bootstrap": "^3.11",
        "drupal/cdn": "^3.2",
        "drupal/ckeditor_markdown": "^1.0",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drupal/devel": "^1.2",
        "drupal/eck": "^1.0@alpha",
        "drupal/examples": "1.x-dev",
        "drupal/features": "^3.7",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/fivestar": "1.x-dev",
        "drupal/flag": "^4.0@alpha",
        "drupal/ga_push": "^1.0@alpha",
        "drupal/google_analytics": "^2.2",
        "drupal/libraries": "^3.0@alpha",
        "drupal/markdown": "^1.2",
        "drupal/nodejs": "^1.1",
        "drupal/number_double": "^1.2",
        "drupal/oauth": "^2.1",
        "drupal/pathauto": "^1.2",
        "drupal/profile": "^1.0@RC",
        "drupal/rate": "^1.0@alpha",
        "drupal/restui": "^1.15",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/services": "^4.0@beta",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplehtmldom": "2.x-dev",
        "drupal/token": "^1.3",
        "drupal/token_filter": "^1.0@beta",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/votingapi": "^3.0@alpha",
        "drupal/votingapi_widgets": "^1.0",
        "drupal/webprofiler": "^2.0@RC",
        "drush/drush": "^9.0.0",
        "htmlpurifier": "^4.10",
        "michelf/php-markdown": "^1.8",
        "simplehtmldom": "^1.5",
        "thomasbachem/php-ga": "^1.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "secure-http": false,
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
